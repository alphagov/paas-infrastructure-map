{
	"compilerOptions": {
		"strict": true,
		"outDir": "dist",
		"module": "commonjs",
		"target": "ES2017",
		"lib": [
			"ES2017"
		],
		"inlineSourceMap": true,
		"inlineSources": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"noImplicitAny": true,
		"isolatedModules": false,
		"pretty": true,
		"allowJs": true,
		"importHelpers": true
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules",
		"dist"
	],
	"types": []
}
